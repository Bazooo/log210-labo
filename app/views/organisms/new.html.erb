<% if @organisme.errors.any? %>
    <div id="error_explanation">
          <ul style="list-style-type:none">
            <% @organisme.errors.full_messages.each do |msg| %>
              <li><%= react_component('Alert', {text: msg}) %></li>
            <% end %>
        </ul>
    </div>
<% end %>
