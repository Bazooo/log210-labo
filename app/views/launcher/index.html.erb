<% @users = User.all %>
<% if current_user.role == "directeur" %>
    <%= link_to 'My Organism(s)'.html_safe, organisms_path() %>
<% end %>
<% if current_user.role == "directeur" || current_user.role == "coordonateur" %>
    <%= link_to 'Referents Organisms'.html_safe, reforganisms_path() %>
<% end %>
<%= react_component('Landing', {users: @users}) %>
<% if current_user.role == "directeur" || current_user.role == "coordonateur" %>
    <%= react_component('LandingCreateUserForm', {token: form_authenticity_token, userRole: current_user.role, roles: User.roles.keys}) %>
<% end %>
